class Solution:
    def countCharacters(self, words: List[str], chars: str) -> int:
        c=Counter(chars) #{a:2,t:1.......}
        total=0
        for word in words:
            w=Counter(word)
            for ch in w:
                if w[ch]>c[ch]:
                    break
            else:
                total+=len(word)
        return total

